page {
  background: #f5f5f5;
}
.swiper-tab {
  height: 80rpx;
  line-height: 80rpx;
  background: #fff;
  color: #a5a5a5;
  white-space: nowrap;
}
.swiper-tab-list {
  float: left;
  font-size: 28rpx;
  margin: 0 30rpx;
}
.on {
  border-bottom:6rpx solid #ff591c;
  color: #ff591c;
}
.navTopList {
  display: flex;
  width: 100%;
  height: 70rpx;
  background: #fff;
  border-bottom: 1rpx solid #BEBEBE	;
  margin-top:5px;
}
 
.navTopList .order-info {
  text-align: center;
}
 
.navTopList .order-info view {
  width: 56rpx;
  height: 56rpx;
  margin-left: 31rpx;
}
 
.navTopList .order-info text {
  font-size: 34rpx;
}
 
.navTopList .order-info text.active {
  color: #ff6662;
}
 
.navTopList .ywc,.navTopList .dfk,.navTopList .yqx {
  flex: 1;
}
 
 
/*订单列表*/
.orderDetails {
  background: #fff;
  border-bottom: 16rpx solid #f5f5f5;
}
 
.orderListTitle {
  height: 56rpx;
  line-height: 56rpx;
  border-bottom: 2rpx solid #f5f5f5;
}
 
.orderListTitle .orderTime {
  padding-right: 50rpx;
  margin-left:10px;
  font-size: 24rpx;
  color: #acacac;
}
 
.orderListTitle .orderStatus {
  float: right;
  margin-right: 20rpx;
  color: #2f7b27;
  font-size: 24rpx;
  color: #ff8c61;
}
 
.orderListDetails {
  display: flex;
  height: 220rpx;
  border-bottom: 2rpx solid #f5f5f5;
}

.orderListDetails .productImg {
  height:200rpx;
  width:30%;
  margin-top: 20rpx;
  margin-left: 20rpx;
}
 
.orderListDetails .productImg image {
  width: 200rpx;
  height: 200rpx;
}
 
.orderListDetails .productInfo {
  width: 70%;
  margin: 40rpx 0  20rpx 30rpx;
}
.productInfo .productName {
  font-size:28rpx;
  color: #585858;
  overflow: hidden;
  margin-bottom:16rpx;
}
.productInfo .productMsg {
  font-size: 24rpx;
  color: #a5a5a5;
}

.productCount {
  height: 64rpx;
  line-height: 64rpx;
  border-bottom: 2rpx solid #f3f3f3;
}
.productCount>view {
  float: right;
  margin-right: 30rpx;
  font-size: 24rpx;
  color: #515151;
}
.productMore {
  height: 100rpx;
}
.productMore view{
  width: 150rpx;
  height: 56rpx;
  border-radius: 14rpx;
  border: 2rpx solid #515151;
  font-size: 24rpx;
  color: #515151;
  line-height: 56rpx;
  text-align: center;
  float: right;
  margin: 24rpx 10rpx;
}
.productMore view:nth-of-type(1){
  width: 150rpx;
  height: 56rpx;
  border-radius: 14rpx;
  border: 2rpx solid #ff8c61;
  font-size: 24rpx;
  color: #ff8c61;
  line-height: 56rpx;
  text-align: center;
  float: right;
  margin: 24rpx 10rpx;
}

.loseimg{
  position: relative;
  top: 140rpx;
  left: 125rpx;
}

.tip{
  position: relative;
  top: 190rpx;
  /* left: 250rpx; */
  text-align: center;
}

.word1{
  font-size: 57.1rpx;
  color: #1e1e1e;
}

.word2{
  font-size: 42.8rpx;
  color: #808080;
  margin-top: 33.3rpx;
}

.jumplist{
  background-color: #ff5414;
  width: 360rpx;
  height: 100rpx;
  position: relative;
  top: 240rpx;
  left: 185rpx;
  border-radius: 59.5rpx;
  text-align: center;
  line-height: 100rpx;
  color: #ffffff;
  font-size: 50rpx;
}