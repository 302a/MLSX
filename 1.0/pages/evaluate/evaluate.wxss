page {
  background: #f5f5f5;
}
.evaluate-box {
  width: 700rpx;
  height: 540rpx;
  background: #fff;
  border-radius: 50rpx;
  margin: 40rpx auto;
}

.evaluate-list {
  width: 100%;
  height: 58rpx;
  margin-top: 40rpx;
}
.left {
  float: left;
  margin: 0 44rpx;
  line-height: 58rpx;
  font-size: 28rpx;
  color: #545454;
}
.right {
  float: left;
}
.star {
  float: left;
  line-height: 58rpx;
  margin: 0 18rpx;
}
.star image{
  width: 50rpx;
  height: 58rpx;
}