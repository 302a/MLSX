/* pages/pintuan_details/pintuan_details.wxss */
page {
  position: absolute;
  overflow-y: hidden;
  width: 100%;
  height: 100%;
}
scroll-view {
  height: 100%;
} 

page,block,view,scroll-view,swiper,movable-view,icon,text,progress,button,checkbox,form,input,label,picker,picker-view,radio,slider,switch,textarea,navigator,audio,image,video,map,canvas,contact-button {
    box-sizing: border-box;
}

.flex-col {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: vertical;
    flex-direction: column;
}

.flex-row {
    display: -webkit-box;
    /* display: -webkit-flex;
    display: flex;
    -webkit-box-orient: horizontal;
    flex-direction: row; */
}

.flex-grow-0 {
    min-width: 0;
    -webkit-box-flex: 0;
    -ms-flex-positive: 0;
    flex-grow: 0;
    -ms-flex-negative: 0;
    flex-shrink: 0;
}

.flex-grow-1 {
    min-width: 0;
    -webkit-box-flex: 1;
    -ms-flex-positive: 1;
    /* flex-grow: 1; */
    -ms-flex-negative: 1;
    flex-shrink: 1;
}

.flex-grow-new {
    min-width: 0;
    -webkit-box-flex: 1;
    -ms-flex-positive: 1;
    flex-grow: 1;
    -ms-flex-negative: 1;
    flex-shrink: 1;
}

.flex-x-center {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: center;
    -ms-flex-pack: center;
    justify-content: center;
}

.flex-y-center {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -ms-flex-align: center;
    -ms-grid-row-align: center;
    align-items: center;
}

.flex-y-bottom {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: end;
    -ms-flex-align: end;
    -ms-grid-row-align: flex-end;
    align-items: flex-end;
}

.service-more image,.goods-attr-more image,.pt-rule-more image,.comment-more image {
    width: 16rpx;
    height: 26rpx;
}

.goods-image-swiper {
    height: 700rpx;
}

.goods-image {
    width: 100%;
    height: 100%;
}

.goods-money {
    height: 110rpx;
    padding-left: 24rpx;
    font-size: 35rpx;
    color: #ffffff;
    /* background-color: #ffffff; */
    background: -webkit-gradient(linear,left top,right bottom,color-stop(0%,#FF8B57),color-stop(140%,#FE5D76));
    background-image: linear-gradient(140deg,#FF8B57,#FE5D76);
}

.goods-money-num {
    font-size: 60rpx;
    line-height: 110rpx;
    font-weight: 500;
    margin: 30rpx 20rpx 0 6rpx;
}

.original-price {
    padding-top: 20rpx;
}

.original-price {
    font-size: 9pt;
    color: #ffffff;
    text-decoration: line-through;
}

.group-descount {
    color: #ff4600;
    height: 30rpx;
    font-size: 7pt;
    margin-top: 10rpx;
}

.group-descount-one {
    padding: 0 12rpx;
    background-color: #ffffff;
    border-radius: 5rpx;
}

.group-descount-center {
    width: 6rpx;
}

.group-descount-tow {
    width: 155rpx;
    font-size: 35rpx;
    margin-left: 10rpx;
    margin-top: 35rpx;

    /* padding: 0 12rpx; */
    /* background-color: #feeeee; */
}

.group-descount-right {
  background-color: #ffdbdb;
  margin-right: 0rpx;
  width: 220rpx;
}

.group-descount-right-end1 {
  color: #ff4600;
  font-size: 23rpx;
  margin-top: 15rpx;
  margin-left: 63rpx;
}

.group-descount-right-end2 {
  color: #ff4600;
  font-size: 36rpx;
  margin-left: 78rpx;
  font-weight: bold;
}

.group-share {
    padding-top: 24rpx;
    padding-right: 15rpx;
    background-color: transparent!important;
    line-height: normal;
}

button::after {
    content: normal!important;
}

.group-share-text {
    font-size: 8pt;
    color: #707070;
}

.goods-name {
    height: 142rpx;
    font-size: 32rpx;
    color: #616161;
    padding: 24rpx;
    background-color: #ffffff;
    line-height: 1.5;
}

.goods-name-bottom {
  font-size: 23rpx;
  margin-top: 18rpx;
}

.goods-name-bottom-1 {
  margin-left: 28rpx;
}

.goods-name-bottom-image {
  float: left;
  width: 20rpx;
  height: 20rpx;
  margin-top: 7rpx;
}

.goods-attr {
    height: 100rpx;
    padding: 0 24rpx;
    background-color: #ffffff;
    border-top: 5rpx solid #f7f7f7;
}

.goods-attr-title {
    width: 128rpx;
    font-size: 13pt;
    color: #353535;
}

.goods-attr-value {
    font-size: 11pt;
    color: #666666;
}

.pt-goods-rule {
