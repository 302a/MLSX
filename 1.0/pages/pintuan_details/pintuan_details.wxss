}

.pt-goods-content-title {
    height: 80rpx;
    background-color: #ffffff;
    padding: 0 24rpx;
    color: #666666;
    font-size: 11pt;
    border-bottom: 1rpx solid #e2e2e2;
}

.pt-goods-details {
    box-sizing: border-box;
    padding: 0 0 110rpx 0;
    background: #fff;
    overflow-x: hidden;
}

