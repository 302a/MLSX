    border-top: 18rpx solid #f7f7f7;
}

.pt-rule-title {
    font-size: 9pt;
    color: #666666;
}

.pt-rule-right {
    font-size: 9pt;
    color: #919191;
    padding-right: 20rpx;
}

.goods-comment {
    padding: 0 24rpx;
    background-color: #ffffff;
    /* margin-top: 16rpx; */
    border-top: 18rpx solid #f7f7f7;
}

.goods-comment-title {
    height: 80rpx;
    font-size: 9pt;
    color: #919191;
}

.pt-goods-content {
    /* margin-top: 16rpx; */
